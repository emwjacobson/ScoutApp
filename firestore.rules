service cloud.firestore {
  match /databases/{database}/documents {
    match /{year}/{regional} {
      allow read: if true;
      allow write: if request.auth.token.admin == true;
      match /{category}/{document=**} {
      	// allow read, write: if request.auth.uid != null;
      	allow read, write: if true;
      }
    }
  }
}
