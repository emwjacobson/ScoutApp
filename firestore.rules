service cloud.firestore {
  match /databases/{database}/documents {
    // match /{document=**} {
    //   allow read, write;
    // }
    match /{year}/{regional} {
      allow read: if true;
      allow write: if request.auth.token.admin == true;
    }
    match /{year}/{regional}/{renameme} {
      allow read: if request.auth != null;    // Untested
      allow write: if request.auth != null;   // Untested
    }
  }
}
